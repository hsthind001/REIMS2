  "validation_rules": [
    {
      "name": "accounting_equation",
      "rule": "total_assets = total_liabilities + total_capital",
      "severity": "critical",
      "tolerance": 0.01,
      "description": "Assets must equal Liabilities + Equity",
      "source": "template_section:fundamental_equation"
    },
    {
      "name": "current_assets_total_validation",
      "rule": "sum(detail_line_items) = total_current_assets",
      "severity": "high",
      "tolerance": 0.01,
      "section": "current_assets",
      "source": "template_section:section_totals"
    },
    {
      "name": "property_equipment_total_validation",
      "rule": "sum(detail_line_items) = total_property_equipment",
      "severity": "high",
      "tolerance": 0.01,
      "section": "property_equipment",
      "source": "template_section:section_totals"
    },
    {
      "name": "other_assets_total_validation",
      "rule": "sum(detail_line_items) = total_other_assets",
      "severity": "high",
      "tolerance": 0.01,
      "section": "other_assets",
      "source": "template_section:section_totals"
    },
    {
      "name": "assets_total_validation",
      "rule": "total_current_assets + total_property_equipment + total_other_assets = total_assets",
      "severity": "critical",
      "tolerance": 0.01,
      "section": "assets",
      "source": "template_section:section_totals"
    },
    {
      "name": "current_liabilities_total_validation",
      "rule": "sum(detail_line_items) = total_current_liabilities",
      "severity": "high",
      "tolerance": 0.01,
      "section": "current_liabilities",
      "source": "template_section:section_totals"
    },
    {
      "name": "long_term_liabilities_total_validation",
      "rule": "sum(detail_line_items) = total_long_term_liabilities",
      "severity": "high",
      "tolerance": 0.01,
      "section": "long_term_liabilities",
      "source": "template_section:section_totals"
    },
    {
      "name": "liabilities_total_validation",
      "rule": "total_current_liabilities + total_long_term_liabilities = total_liabilities",
      "severity": "critical",
      "tolerance": 0.01,
      "section": "liabilities",
      "source": "template_section:section_totals"
    },
    {
      "name": "all_required_sections_present",
      "check_data": [
        "assets",
        "liabilities",
        "capital"
      ],
      "severity": "critical",
      "source": "template_section:quality_checks"
    },
    {
      "name": "required_totals_extracted",
      "check_data": [
        "1999-0000",
        "2999-0000",
        "3999-0000",
        "3999-9000"
      ],
      "severity": "critical",
      "source": "template_section:quality_checks"
    },
    {
      "name": "no_duplicate_accounts",
      "check_data": "unique(property_id, period_id, account_code)",
      "severity": "high",
      "source": "template_section:quality_checks"
    },
    {
      "name": "reasonable_amounts",
      "check_data": "total_assets > 0 AND total_liabilities >= 0",
      "severity": "medium",
      "source": "template_section:quality_checks"
    },
    {
      "name": "contra_accounts_negative",
